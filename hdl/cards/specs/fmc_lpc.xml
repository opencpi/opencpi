  <signal bidirectional='LA13_N'/>      <!-- Pin D18 -->
  <signal bidirectional='LA17_P_CC'/>   <!-- Pin D20 -->
  <signal bidirectional='LA17_N_CC'/>   <!-- Pin D21 -->
  <signal bidirectional='LA23_P'/>      <!-- Pin D23 -->
  <signal bidirectional='LA23_N'/>      <!-- Pin D24 -->
  <signal bidirectional='LA26_P'/>      <!-- Pin D26 -->
  <signal bidirectional='LA26_N'/>      <!-- Pin D27 -->

  <signal output='DP0_C2M_P'/>          <!-- Pin C2 -->
  <signal output='DP0_C2M_N'/>          <!-- Pin C3 -->
  <signal input='DP0_M2C_P'/>           <!-- Pin C6 -->
  <signal input='DP0_M2C_N'/>           <!-- Pin C7 -->
  <signal bidirectional='LA06_P'/>      <!-- Pin C10 -->
  <signal bidirectional='LA06_N'/>      <!-- Pin C11 -->
