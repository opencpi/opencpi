<!--
  The slot type declaration for FMC (VITA 57.1) Low Pin Count connector
  Signals ordered and named per connector diagram in Table 3/Page 48 of ANSI/VITA spec
  Approved July 2008, Revised February 2010.
  "Input" is from the view of the carrier, i.e. input is from mezzanine card to carrier
  Ground pins ommitted.  Power supply pins ommitted.  JTAG signals omitted.
-->
<SlotType name='fmc_lpc'>
  <signal input='VREF_A_M2C'/>          <!-- Pin H1 -->
  <signal input='PRSNT_M2C_L'/>         <!-- Pin H2 -->
  <signal input='CLK0_M2C_P'/>          <!-- Pin H4 -->
  <signal input='CLK0_M2C_N'/>          <!-- Pin H5 -->
  <signal bidirectional='LA02_P'/>      <!-- Pin H7 -->
  <signal bidirectional='LA02_N'/>      <!-- Pin H8 -->
  <signal bidirectional='LA04_P'/>      <!-- Pin H10 -->
  <signal bidirectional='LA04_N'/>      <!-- Pin H11 -->
  <signal bidirectional='LA07_P'/>      <!-- Pin H13 -->
  <signal bidirectional='LA07_N'/>      <!-- Pin H14 -->
  <signal bidirectional='LA11_P'/>      <!-- Pin H16 -->
  <signal bidirectional='LA11_N'/>      <!-- Pin H17 -->
  <signal bidirectional='LA15_P'/>      <!-- Pin H19 -->
  <signal bidirectional='LA15_N'/>      <!-- Pin H20 -->
  <signal bidirectional='LA19_P'/>      <!-- Pin H22 -->
  <signal bidirectional='LA19_N'/>      <!-- Pin H23 -->
  <signal bidirectional='LA21_P'/>      <!-- Pin H25 -->
  <signal bidirectional='LA21_N'/>      <!-- Pin H26 -->
  <signal bidirectional='LA24_P'/>      <!-- Pin H28 -->
  <signal bidirectional='LA24_N'/>      <!-- Pin H29 -->
  <signal bidirectional='LA28_P'/>      <!-- Pin H31 -->
  <signal bidirectional='LA28_N'/>      <!-- Pin H32 -->
  <signal bidirectional='LA30_P'/>      <!-- Pin H34 -->
  <signal bidirectional='LA30_N'/>      <!-- Pin H35 -->
  <signal bidirectional='LA32_P'/>      <!-- Pin H37 -->
  <signal bidirectional='LA32_N'/>      <!-- Pin H38 -->

  <signal input='CLK1_M2C_P'/>          <!-- Pin G2 -->
  <signal input='CLK1_M2C_N'/>          <!-- Pin G3 -->
  <signal bidirectional='LA00_P_CC'/>   <!-- Pin G6 -->
  <signal bidirectional='LA00_N_CC'/>   <!-- Pin G7 -->
  <signal bidirectional='LA03_P'/>      <!-- Pin G9 -->
  <signal bidirectional='LA03_N'/>      <!-- Pin G10 -->
  <signal bidirectional='LA08_P'/>      <!-- Pin G12 -->
  <signal bidirectional='LA08_N'/>      <!-- Pin G13 -->
  <signal bidirectional='LA12_P'/>      <!-- Pin G15 -->
  <signal bidirectional='LA12_N'/>      <!-- Pin G16 -->
  <signal bidirectional='LA16_P'/>      <!-- Pin G18 -->
  <signal bidirectional='LA16_N'/>      <!-- Pin G19 -->
  <signal bidirectional='LA20_P'/>      <!-- Pin G21 -->
  <signal bidirectional='LA20_N'/>      <!-- Pin G22 -->
  <signal bidirectional='LA22_P'/>      <!-- Pin G24 -->
  <signal bidirectional='LA22_N'/>      <!-- Pin G25 -->
  <signal bidirectional='LA25_P'/>      <!-- Pin G27 -->
  <signal bidirectional='LA25_N'/>      <!-- Pin G28 -->
  <signal bidirectional='LA29_P'/>      <!-- Pin G30 -->
  <signal bidirectional='LA29_N'/>      <!-- Pin G31 -->
  <signal bidirectional='LA31_P'/>      <!-- Pin G33 -->
  <signal bidirectional='LA31_N'/>      <!-- Pin G34 -->
  <signal bidirectional='LA33_P'/>      <!-- Pin G36 -->
  <signal bidirectional='LA33_N'/>      <!-- Pin G37 -->


  <!-- irrelevant to carrier FPGA <signal output='PG_C2M'/>  Pin D1 -->
  <signal input='GBTCLK0_M2C_P'/>       <!-- Pin D4 -->
  <signal input='GBTCLK0_M2C_N'/>       <!-- Pin D5 -->
  <signal bidirectional='LA01_P_CC'/>   <!-- Pin D8 -->
  <signal bidirectional='LA01_N_CC'/>   <!-- Pin D9 -->
  <signal bidirectional='LA05_P'/>      <!-- Pin D11 -->
  <signal bidirectional='LA05_N'/>      <!-- Pin D12 -->
